{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/../../ioBroker/node_modules/iobroker.yahka/main.js",
            "stopOnEntry": false,
            "args": [
                "0",
                "debug",
                "--force"
            ],
            "cwd": "${workspaceRoot}/../../ioBroker/node_modules/iobroker.yahka",
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/../../ioBroker/node_modules/iobroker.yahka/**/*.js"
            ],
            "sourceMapPathOverrides": {
                "webpack:///./~/*": "${workspaceRoot}/src/node_modules/*",
                "webpack:///./*": "${workspaceRoot}/src/*",
                "webpack:///*": "*"
            }
        }
    ]
}